{
  "name": "Indus Drupal 8  Angular 2",
  "version": "0.1.0",
  "description": "Drupal 8 distribution for Angular 2",
  "type": "project",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "FauconV",
      "role": "Developer"
    }
  ],
  "config": {
    "bin-dir": "scripts"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "mouf/nodejs-installer": "^1.0",
    "composer/installers": "^1.0.20",
    "cweagans/composer-patches": "~1.0",
    "drupal-composer/drupal-scaffold": "^2.0.1",
    "drupal/core": "~8.0",
    "drush/drush": "~8.0",
    "drupal/console": "^1.0",
    "drupal/admin_menu": "3.x-dev",
    "drupal/composer_manager": "1.x-dev"
  },
  "require-dev": {
    "behat/mink": "~1.7",
    "behat/mink-goutte-driver": "~1.2",
    "jcalderonzumba/gastonjs": "~1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8",
    "symfony/css-selector": "~2.8",
    "drupal/generator_ui": "1.x-dev",
    "drupal/edit_ui": "1.x-dev",
    "drupal/libraries_ui": "1.x-dev",
    "drupal/breakpoints_ui": "1.x-dev",
    "drupal/restui": "^1.12",
    "drupal/module_builder": "3.x-dev"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "extra": {
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "mouf": {
      "nodejs": {
        "version": "*",
        "targetDir": "scripts",
        "forceLocal": true
      }
    }
  }
}
