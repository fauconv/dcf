
#
# remove existing file
#
function site_remove {
  if [ -e ${ABS_CONFIG_PATH}/settings-${1}.php ]; then
    setRight dev
    rm ${ABS_CONFIG_PATH}/settings-${1}.php ${ABS_CONFIG_PATH}/mock-${1}.xml ${ABS_CONFIG_PATH}/masquerade-${1}.xml
    echo -e "Removing ${1}...                                \e[32m\e[1m[ok]\e[0m"
  fi
}
